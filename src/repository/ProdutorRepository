const Produtor = require('../models/Produtor');

const createProdutor = async (dados) => {
  return await Produtor.create(dados);
};

const listProdutores = async () => {
  return await Produtor.findAll();
};

const finfById = async (id) => {
  return await Produtor.findByPk(id);
};

module.exports = {
  createProdutor,
  listProdutores,
  finfById,
};
